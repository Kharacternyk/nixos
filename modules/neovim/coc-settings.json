{
  "inlayHint.enable": false,
  "flutter.provider.hot-reload": false,
  "dart.showTodos": false,
  "languageserver": {
    "nix": {
      "command": "nixd",
      "filetypes": ["nix"]
    },
    "typst": {
      "command": "typst-lsp",
      "filetypes": ["typst"]
    }
  },
  "pyright.organizeimports.provider": "ruff",
  "python.formatting.provider": "ruff",
  "suggest.noselect": true
}
